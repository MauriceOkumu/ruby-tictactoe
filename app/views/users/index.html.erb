<h1>Home page</h1>

<h3>These are all the users currently in the Database</h3>

<table>
  <tr>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Email</th>
  </tr>
  <% @users.each do |user| %>
   <tr>
    <td><%=user.first_name%></td>
    <td><%=user.last_name%></td>
    <td><%=user.email%></td>
    <td><%= link_to('show user', user_path(user),:class => 'action show') %></td>
    <td><%= link_to('edit user', edit_user_path(user),:class => 'action edit') %></td>
  </tr>
  <% end %>
</table>